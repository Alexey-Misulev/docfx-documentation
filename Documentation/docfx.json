{
  "metadata": [
    {
      "src": [
        {
          "files": [ "Assets/Scripts/**/*.cs" ],
          "src": "../"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      { "files": [ "toc.yml", "index.md" ] },
      { "files": [ "api/**.yml" ], "src": "api" }
    ],
    "dest": "_site",
    "template": [ "default" ]
  }
}