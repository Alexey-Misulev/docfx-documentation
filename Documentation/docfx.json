{
  "metadata": [
    {
      "src": [
        {
          "files": [ "Assets/Scripts/**/*.cs" ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      { "files": [ "Documentation/toc.yml", "Documentation/index.md" ] },
      { "files": [ "api/**.yml" ], "src": "api" }
    ],
    "dest": "Documentation/_site",
    "template": [ "default", "modern" ],
    "globalMetadata": {
      "_appTitle": "My Docs"
    }
  }
}